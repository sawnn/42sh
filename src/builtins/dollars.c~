#include "../../include/shell.h"

char	*dollars(char *str, t_set *list, node *env)
{
	t_set *tmp = list;
	node *env = env;

	while (tmp) {
		if (strcmp(tmp->op, str) == 0)
			return (tmp->value);
		tmp = tmp->next;
	}
	while (env) {
		if (my_strncmp(env->str, str, my_strlen(str)) == 0)
			return ();
		tmp = tmp->next;
	}
	printf("%s: Undefined variable.\n", str);
	return (NULL);
}
